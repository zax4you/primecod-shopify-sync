{
  "functions": {
    "api/*.js": {
      "runtime": "@vercel/node"
    },
    "api/sync-orders.js": {
      "maxDuration": 60
    }
  },
  "rewrites": [
    {
      "source": "/",
      "destination": "/api/index"
    }
  ],
  "crons": [
    {
      "path": "/api/sync-orders",
      "schedule": "0 22 * * *"
    }
  ]
}
